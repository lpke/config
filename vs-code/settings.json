{
  // ==================================
  // == VSCode General
  // ==================================
  // Telemetry
  "telemetry.telemetryLevel": "off",
  "workbench.enableExperiments": false,
  // Blank / New Editor
  "workbench.startupEditor": "newUntitledFile",
  "workbench.tips.enabled": false,
  // ==================================
  // == Editor Settings
  // ==================================
  // Text formatting
  "editor.tabSize": 2,
  "editor.indentSize": "tabSize",
  "files.trimFinalNewlines": false,
  "editor.fontLigatures": true,
  // Explorer
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "explorer.compactFolders": false,
  "workbench.editor.closeOnFileDelete": true,
  // Behaviour
  "editor.wordWrap": "on",
  "git.confirmSync": false,
  "editor.matchBrackets": "never",
  "editor.copyWithSyntaxHighlighting": false,
  "editor.insertSpaces": true,
  "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
  // Hints and extra UI
  "editor.parameterHints.enabled": false,
  "editor.lightbulb.enabled": false,
  // Suggestions
  "editor.inlineSuggest.enabled": false,
  "editor.suggest.showReferences": false,
  "editor.suggest.showSnippets": false,
  "editor.suggest.showInterfaces": false,
  "editor.suggest.showModules": false,
  "editor.suggest.showClasses": false,
  // Tab completion
  "editor.tabCompletion": "onlySnippets",
  "editor.snippetSuggestions": "top",
  "editor.acceptSuggestionOnEnter": "smart",
  "editor.quickSuggestionsDelay": 0,
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.suggestOnTriggerCharacters": false,
  "editor.suggest.localityBonus": true,
  // GitHub Copilot
  "github.copilot.editor.enableAutoCompletions": false,
  "github.copilot.enable": {
    "*": true,
    "plaintext": true,
    "markdown": false,
    "scminput": false
  },
  "github.copilot.advanced": {},
  // CodeLens (first option will override all others)
  "editor.codeLens": false,
  "csharp.referencesCodeLens.enabled": false,
  // Selection
  "editor.roundedSelection": false,
  "editor.selectionHighlight": false,
  "editor.occurrencesHighlight": false,
  // View
  "editor.lineNumbers": "relative",
  "workbench.editor.wrapTabs": true,
  "workbench.editor.pinnedTabSizing": "compact",
  "workbench.editor.tabSizing": "shrink",
  "editor.links": false,
  // Cursor
  "editor.cursorStyle": "line",
  "editor.cursorBlinking": "solid",
  // Title Bar
  "window.title": "${dirty}${activeEditorShort}${separator}${rootName}${separator}${profileName}",
  "window.titleBarStyle": "custom",
  "window.customMenuBarAltFocus": false,
  "window.enableMenuBarMnemonics": false,
  "window.commandCenter": false,
  // Minimap
  "editor.minimap.maxColumn": 95,
  "editor.minimap.showSlider": "always",
  // Breadcrumbs
  "breadcrumbs.enabled": true,
  "breadcrumbs.filePath": "on",
  "breadcrumbs.symbolPath": "last",
  "breadcrumbs.icons": false,
  // Integrated Terminal Behaviour
  "terminal.integrated.localEchoLatencyThreshold": 100,
  "terminal.integrated.localEchoStyle": "dim",
  "terminal.integrated.drawBoldTextInBrightColors": true,
  "terminal.integrated.fontWeightBold": "normal",
  "terminal.integrated.gpuAcceleration": "on",
  "powershell.integratedConsole.showOnStartup": false,
  "editor.quickSuggestions": {
    "comments": "off",
    "strings": "off",
    "other": "off"
  },
  "terminal.integrated.commandsToSkipShell": [
    "workbench.action.closePanel",
    "workbench.action.focusAuxiliaryBar",
    "workbench.action.focusSideBar"
  ],
  // File Exclusions
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/node_modules": true
  },
  // ==================================
  // == Font Settings
  // ==================================
  // EDITOR
  "editor.inlineHints.fontFamily": "'JetBrains Mono', 'Fira Code', Monaco, Menlo, 'Courier New', monospace",
  "editor.fontFamily": "'JetBrains Mono', 'Fira Code', Monaco, Menlo, 'Courier New', monospace",
  "editor.fontSize": 16,
  "editor.lineHeight": 24,
  // DEBUG CONSOLE
  "debug.console.fontFamily": "'JetBrains Mono', 'Fira Code', Monaco, Menlo, 'Courier New', monospace",
  "debug.console.fontSize": 16,
  "debug.console.lineHeight": 24,
  // TERMINAL
  "terminal.integrated.fontFamily": "'JetBrainsMono Nerd Font Mono','JetBrains Mono', 'Fira Code', Monaco, Menlo, 'Courier New', monospace",
  "terminal.integrated.fontSize": 16,
  "terminal.integrated.lineHeight": 1.2,
  // ==================================
  // == Language Settings
  // ==================================
  // File Associations (VS Code)
  "files.associations": {
    "*.tsx": "typescriptreact",
    "*.jsx": "javascriptreact",
    "*.ahk": "ahk2",
    ".env": "env"
  },
  // JavaScript
  "javascript.validate.enable": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "javascript.format.semicolons": "insert",
  "javascript.preferences.quoteStyle": "single",
  "javascript.preferences.renameMatchingJsxTags": true,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  // TypeScript
  "typescript.validate.enable": true,
  "typescript.tsserver.maxTsServerMemory": 8000,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "typescript.format.semicolons": "insert",
  "typescript.preferences.quoteStyle": "single",
  "typescript.preferences.renameMatchingJsxTags": true,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  // Python
  "python.pythonPath": "/usr/bin/python3",
  "python.showStartPage": false,
  // CSS
  "less.lint.emptyRules": "ignore",
  "scss.lint.emptyRules": "ignore",
  "css.lint.emptyRules": "ignore",
  "[tailwindcss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[postcss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[less]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  // JSON
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  // HTML
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  // AutoHotkey
  "[ahk]": {
    "editor.defaultFormatter": "mark-wiemer.vscode-autohotkey-plus-plus",
    "editor.insertSpaces": true,
    "editor.tabSize": 2
  },
  "[ahk2]": {
    "editor.defaultFormatter": "mark-wiemer.vscode-autohotkey-plus-plus",
    "editor.insertSpaces": true,
    "editor.tabSize": 2
  },
  // Markdown
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  // Graphql
  "[graphql]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  // Vue
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  // ==================================
  // == Extension Settings
  // ==================================
  // Prettier (follows airbnb js styleguide)
  // comment format: // [default value], [.editorconfig setting]
  "prettier.printWidth": 80, // 80, max_line_length
  "prettier.tabWidth": 2, // 2, indent_size | tab_width
  "prettier.useTabs": false, // false, indent_style
  "prettier.semi": true, // true
  "prettier.singleQuote": true, // false
  "prettier.quoteProps": "as-needed", // 'as-needed'
  "prettier.jsxSingleQuote": false, // false
  "prettier.trailingComma": "all", // 'all'
  "prettier.bracketSpacing": true, // true
  "prettier.bracketSameLine": false, // false
  "prettier.arrowParens": "always", // 'always'
  "prettier.requirePragma": false, // false
  "prettier.insertPragma": false, // false
  "prettier.proseWrap": "preserve", // 'preserve'
  "prettier.htmlWhitespaceSensitivity": "css", // 'css'
  "prettier.vueIndentScriptAndStyle": false, // false
  "prettier.endOfLine": "lf", // 'lf', end_of_line
  "prettier.embeddedLanguageFormatting": "auto", // 'auto'
  "prettier.singleAttributePerLine": false, // false
  // Emmet
  "emmet.showSuggestionsAsSnippets": true,
  "emmet.triggerExpansionOnTab": false,
  "tailwindCSS.emmetCompletions": true,
  "emmet.includeLanguages": {
    "postcss": "css"
  }, // for custom snippets
  // Highlight Matching Tag
  "highlight-matching-tag.highlightSelfClosing": false,
  "highlight-matching-tag.styles": {
    "opening": {
      "name": {
        "underline": "#D4D4D4"
      }
    }
  },
  // Auto Rename Tag
  "auto-rename-tag.activationOnLanguage": [
    "html",
    "javascriptreact",
    "typescriptreact"
  ],
  // Subtle Match Brackets / Colorization
  "subtleBrackets.parse": false,
  "subtleBrackets.style": {
    "borderWidth": "1px",
    "borderStyle": "none none solid none",
    "borderColor": "#D4D4D4"
  },
  // GitLens
  "gitlens.codeLens.enabled": false,
  "gitlens.currentLine.enabled": false,
  "gitlens.hovers.enabled": false,
  "gitlens.hovers.currentLine.over": "annotation",
  "gitlens.statusBar.reduceFlicker": true,
  "gitlens.menus": {
    "editor": {
      "blame": false,
      "clipboard": true,
      "compare": true,
      "history": false,
      "remote": false
    },
    "editorGroup": false,
    "editorTab": {
      "clipboard": true,
      "compare": true,
      "history": true,
      "remote": true
    },
    "explorer": {
      "clipboard": true,
      "compare": true,
      "history": true,
      "remote": true
    },
    "scm": {
      "authors": true
    },
    "scmGroupInline": {
      "stash": true
    },
    "scmGroup": {
      "compare": true,
      "openClose": true,
      "stash": true
    },
    "scmItem": {
      "clipboard": true,
      "compare": true,
      "history": true,
      "remote": false,
      "stash": true
    }
  },
  "gitlens.views.branches.files.layout": "list",
  "gitlens.views.branches.branches.layout": "list",
  "gitlens.views.branches.showBranchComparison": false,
  "gitlens.views.showRelativeDateMarkers": false,
  "gitlens.views.searchAndCompare.files.layout": "list",
  "gitlens.views.commits.showBranchComparison": false,
  "gitlens.views.commits.files.layout": "list",
  // Code Runner
  "code-runner.enableAppInsights": false,
  "code-runner.preserveFocus": true,
  "code-runner.clearPreviousOutput": true,
  "code-runner.temporaryFileName": "_TEMP",
  "code-runner.showRunIconInEditorTitleMenu": false,
  // Markdown Preview
  "markdown-preview-enhanced.scrollSync": true,
  "markdown-preview-enhanced.previewTheme": "atom-material.css",
  // NPM Intellisense
  "npm-intellisense.importQuotes": "\"",
  // Multi Command (custom commands)
  "multiCommand.commands": [
    {
      "command": "multiCommand.clearTerminal",
      "sequence": [
        "workbench.action.terminal.clear",
        "workbench.action.focusActiveEditorGroup"
      ]
    },
    {
      "command": "multiCommand.openFocusSidebar",
      "sequence": [
        "workbench.action.toggleSidebarVisibility",
        "workbench.action.focusSideBar"
      ]
    },
    {
      "command": "multiCommand.cycleProjectChecking",
      "sequence": [
        "settings.cycle.projectCheckingStatus",
        "settings.cycle.projectChecking"
      ]
    }
  ],
  // Settings Cycler (shortcuts to cycle/toggle settings)
  // use in keybindings.json as command: `settings.cycle.<id>`
  "settings.cycle": [
    {
      "id": "copilotAutoSuggestions",
      "overrideWorkspaceSettings": true,
      "values": [
        {
          "github.copilot.editor.enableAutoCompletions": false,
          "editor.inlineSuggest.enabled": false
        },
        {
          "github.copilot.editor.enableAutoCompletions": true,
          "editor.inlineSuggest.enabled": true
        }
      ]
    },
    {
      "id": "projectChecking",
      "overrideWorkspaceSettings": true,
      "values": [
        {
          "typescript.tsserver.experimental.enableProjectDiagnostics": false
        },
        {
          "typescript.tsserver.experimental.enableProjectDiagnostics": true
        }
      ]
    },
    {
      "id": "projectCheckingStatus",
      "overrideWorkspaceSettings": true,
      "values": [
        {
          "statusbar_command.commands": [
            {
              "text": "$(check)",
              "tooltip": "TS: Checking only open files",
              "alignment": "left",
              "command": "multiCommand.cycleProjectChecking",
              "priority": 100
            }
          ]
        },
        {
          "statusbar_command.commands": [
            {
              "text": "$(check-all)",
              "tooltip": "TS: Checking all files",
              "alignment": "left",
              "command": "multiCommand.cycleProjectChecking",
              "priority": 100
            }
          ]
        }
      ]
    }
  ],
  // Todo Tree
  "todo-tree.tree.showScanModeButton": false,
  "todo-tree.general.tags": ["TODO", "UPDATE", "FIXME", "TEMP"],
  "todo-tree.highlights.defaultHighlight": {
    "icon": "issue-opened",
    "type": "whole-line",
    "foreground": "#767080",
    "background": "#e08da5",
    "opacity": 10,
    "iconColour": "#e08da5"
  },
  "todo-tree.highlights.customHighlight": {
    "TODO": {
      "icon": "dot-fill",
      "gutterIcon": true,
      "type": "whole-line",
      "foreground": "#767080",
      "background": "#4AB4C4",
      "opacity": 6,
      "iconColour": "#4AB4C4"
    },
    "UPDATE": {
      "icon": "dot-fill",
      "gutterIcon": true,
      "type": "whole-line",
      "foreground": "#767080",
      "background": "#E566BA",
      "opacity": 10,
      "iconColour": "#E566BA"
    },
    "FIXME": {
      "icon": "alert",
      "gutterIcon": true,
      "type": "whole-line",
      "foreground": "#767080",
      "background": "#e65a6e",
      "opacity": 10,
      "iconColour": "#e65a6e"
    },
    "TEMP": {
      "icon": "dot-fill",
      "gutterIcon": true,
      "type": "tag-and-comment",
      "foreground": "#767080",
      "background": "#FADB82",
      "opacity": 8,
      "iconColour": "#FADB82"
    }
  },
  // statusbar-commands
  "statusbar_command.commands": [
    {
      "text": "$(check)",
      "tooltip": "TS: Checking only open files",
      "alignment": "left",
      "command": "multiCommand.cycleProjectChecking",
      "priority": 100
    }
  ],
  // ESLint
  "eslint.runtime": "node",
  "eslint.options": {},
  "eslint.lintTask.enable": true,
  // ==================================
  // == Theme / Colour Settings
  // ==================================
