      "before": ["z", "t"],
      "after": ["z", "t", "5", "<C-y>"]
    },
    {
      "before": ["z", "b"],
      "after": ["z", "b", "5", "<C-e>"]
    },
    {
      "before": ["z", "u"],
      "after": ["1", "0", "<C-y>"]
    },
    {
      "before": ["z", "d"],
      "after": ["1", "0", "<C-e>"]
    },
