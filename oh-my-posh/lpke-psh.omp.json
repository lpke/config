{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "final_space": false,
  "osc99": true,
  "console_title": true,
  "console_title_style": "template",
  "console_title_template": "{{if .Root}} ‚ö° {{end}}{{.Folder | replace \"~\" \"üèö\" }} @ {{.Host}}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "newLine": true,
      "segments": [
        {
          "type": "root",
          "style": "plain",
          "foreground": "#FFEB3B",
          "background": "#a313a8",
          "properties": {
            "root_icon": "‚ö°"
          }
        },
        {
          "type": "os",
          "style": "plain",
          "foreground": "#ffffff",
          "background": "transparent"
        },
        {
          "type": "path",
          "style": "diamond",
          "leading_diamond": "<transparent,#01579B>\uE0B0</>",
          "trailing_diamond": "\uE0B0",
          "foreground": "#ffffff",
          "background": "#01579B",
          "properties": {
            "home_icon": "\uF7DB",
            "folder_icon": "\uF6D7",
            "folder_separator_icon": "<transparent> \uE0BD </>",
            "style": "agnoster_short"
          }
        },
        {
          "type": "git",
          "style": "diamond",
          "leading_diamond": "<transparent,#29315A>\uE0B0</>",
          "trailing_diamond": "\uE0B0",
          "foreground": "#b6def3",
          "background": "#29315A",
          "properties": {
            "branch_icon": "<#b6def3>\uf418\u2009</>",
            "color_background": false,
            "display_status": true,
            "display_branch_status": true,
            "branch_gone_icon": "\u2262",
            "branch_identical_icon": "\u2261",
            "branch_ahead_icon": "\u2191",
            "branch_behind_icon": "\u2193",
            "status_separator_icon": "|",
            "local_working_icon": "",
            "local_staged_icon": "",
            "stash_count_icon": "",
            "worktree_count_icon": "",
            "status_colors_enabled": true,
            "display_stash_count": true,
            "working_color": "#c7c7c7",
            "staging_color": "#a3ecb9",
            "ahead_and_behind_color": "#FFCC80",
            "behind_color": "#B388FF",
            "ahead_color": "#B388FF"
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "session",
          "style": "diamond",
          "trailing_diamond": "",
          "leading_diamond": "\uE0C5",
          "foreground": "#43CCEA",
          "background": "#29315A",
          "properties": {
            "postfix": "<transparent> </>",
            "prefix": "  ",
            "user_info_separator": "<transparent> / </>"
          }
        },
        {
          "type": "executiontime",
          "style": "plain",
          "foreground": "#ffffff",
          "background": "#49404f",
          "properties": {
            "prefix": "<transparent, #29315A>\uE0BA</><transparent>\uE0BC </>",
            "threshold": 5000,
            "style": "round"
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "exit",
          "style": "plain",
          "foreground": "#43CCDF",
          "properties": {
            "prefix": "\u276F",
            "always_enabled": true,
            "error_color": "#E06C75",
            "display_exit_code": false
          }
        }
      ]
    }
  ]
}
