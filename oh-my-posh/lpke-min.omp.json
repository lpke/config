{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 2,
  "palettes": {
    "template": "{{ if eq .Shell \"zsh\" }}zsh{{ else if eq .Shell \"cmd\" }}cmd{{ else }}psh{{ end }}",
    "list": {
      "psh": {
        "root": "yellow",
        "session": "cyan",
        "path": "green",
        "git": "blue",
        "git-no-remote": "#ebbcba",
        "git-ahead": "#789da7",
        "git-behind": "#9580b2",
        "git-w-changed": "#6e6a86",
        "git-s-changed": "#908caa",
        "git-stash": "#31748f",
        "time": "magenta",
        "prompt": "yellow",
        "prompt-error": "red"
      },
      "cmd": {
        "root": "yellow",
        "session": "cyan",
        "path": "green",
        "git": "blue",
        "git-no-remote": "#ebbcba",
        "git-ahead": "#789da7",
        "git-behind": "#9580b2",
        "git-w-changed": "#6e6a86",
        "git-s-changed": "#908caa",
        "git-stash": "#31748f",
        "time": "magenta",
        "prompt": "yellow",
        "prompt-error": "red"
      },
      "zsh": {
        "root": "yellow",
        "session": "cyan",
        "path": "green",
        "git": "blue",
        "git-no-remote": "#ebbcba",
        "git-ahead": "#789da7",
        "git-behind": "#9580b2",
        "git-w-changed": "#6e6a86",
        "git-s-changed": "#908caa",
        "git-stash": "#31748f",
        "time": "magenta",
        "prompt": "yellow",
        "prompt-error": "red"
      }
    }
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "root",
          "foreground": "p:root",
          "style": "plain",
          "template": "# "
        },
        {
          "type": "session",
          "foreground": "p:session",
          "style": "plain",
          "template": "{{ if .SSHSession }}{{ .UserName }}@{{ .HostName }} {{ end }}"
        },
        {
          "type": "path",
          "foreground": "p:path",
          "style": "plain",
          "properties": {
            "style": "agnoster_short",
            "max_depth": 2
          },
          "template": "{{ .Path }} "
        },
        {
          "type": "git",
          "foreground": "p:git",
          "style": "plain",
          "properties": {
            "branch_icon": "",
            "branch_identical_icon": "",
            "branch_ahead_icon": "Γåæ",
            "branch_behind_icon": "Γåô",
            "branch_gone_icon": "Γêà",
            "branch_max_length": 0,
            "truncate_symbol": "",
            "commit_icon": "",
            "tag_icon": "",
            "rebase_icon": "",
            "cherry_pick_icon": "",
            "revert_icon": "",
            "merge_icon": "",
            "no_commits_icon": "",
            "github_icon": "",
            "gitlab_icon": "",
            "bitbucket_icon": "",
            "azure_devops_icon": "",
            "codecommit_icon": "",
            "git_icon": "",
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_bare_info": true,
            "fetch_worktree_count": true,
            "fetch_stash_count": true
          },
          "template": "{{ .HEAD }}{{if .UpstreamGone }} <p:git-no-remote>Γêà</>{{ end }}{{if .Ahead }} <p:git-ahead>Γåæ{{ .Ahead }}</>{{ end }}{{if .Behind }}<p:git-behind>Γåô{{ .Behind }}</>{{ end }}<p:git-s-changed>{{ if .Staging.Changed }} {{ end }}{{ if .Staging.Added }}+{{ .Staging.Added }}{{ end }}{{ if .Staging.Modified }}~{{ .Staging.Modified }}{{ end }}{{ if .Staging.Deleted }}-{{ .Staging.Deleted }}{{ end }}</><p:git-w-changed>{{ if .Working.Changed }} {{ end }}{{ if .Working.Untracked }}?{{ .Working.Untracked }}{{ end }}{{ if .Working.Modified }}~{{ .Working.Modified }}{{ end }}{{ if .Working.Deleted }}-{{ .Working.Deleted }}{{ end }}</>{{ if gt .StashCount 0}} <p:git-stash>ΓÅÅ{{ .StashCount }}</>{{ end }}{{ if gt .WorktreeCount 0}} {{ .WorktreeCount }}{{ end }} "
        },
        {
          "type": "executiontime",
          "foreground": "p:time",
          "style": "plain",
          "properties": {
            "style": "round",
            "threshold": 60000
          },
          "template": "{{ .FormattedMs }} "
        },
        {
          "type": "status",
          "foreground": "p:prompt",
          "foreground_templates": [
            "{{ if gt .Code 0 }}p:prompt-error{{ end }}"
          ],
          "style": "plain",
          "properties": {
            "always_enabled": true
          },
          "template": "\u276f "
        }
      ]
    }
  ]
}
