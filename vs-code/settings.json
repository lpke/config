    {
      "before": ["d", "b"],
      "after": ["v", "b", "d"]
    },
    {
      "before": ["c", "b"],
      "after": ["v", "b", "c"]
    },
    // Toggle Boolean
    {
      "before": ["c", "i", "b"],
      "commands": [
        {
          "command": "extension.toggleBool"
        }
      ]
    },
